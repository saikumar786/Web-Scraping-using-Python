  {
   "cell_type": "code",
   "execution_count": 105,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Looping through all results\n",
    "for result in results_final:\n",
    "    data = result.find_all('td')\n",
    "    #Extracting each element to a variable\n",
    "    rank = data[0].text\n",
    "    company_name = data[1].find('span').text\n",
    "    webpage = data[1].find('a').get('href')\n",
    "    location = data[2].text\n",
    "    year_end = data[3].text\n",
    "    ann_sales = data[4].text\n",
    "    sales = data[5].text\n",
    "    staff = data[6].text\n",
    "    comments = data[7].text\n",
    "    \n",
    "    rows.append([rank, company_name, webpage, location, year_end, ann_sales, sales, staff, comments])\n",
    "    if len(data) == 0:\n",
    "        continue"
   ]
  },
